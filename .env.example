# Rename this file to .env and fill in the values.
# Get your Gemini API key from Google AI Studio: https://aistudio.google.com/app/apikey
GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# This is a secret key for Flask sessions and other security features.
# You can generate a good one with: python -c 'import secrets; print(secrets.token_hex())'
FLASK_SECRET_KEY="YOUR_FLASK_SECRET_KEY"

# This is the API key that clients must provide to use the /api/chat endpoint.
# You can use a simple string or generate a secure one.
API_KEY="YOUR_AGENT_API_KEY"
